
package exercicios;

import java.util.Scanner;

/**
 *
 * @author rafael.eraquino
 */
public class LacoExe1 {
    public static void main(String[]args){
     Scanner leitor = new Scanner(System.in);
        int num;
        
        System.out.println("digite um numero: ");
        num = leitor.nextInt();
        
        for(int i = 0; i<num; i++){
        
            System.out.println(i);
        
        }
}}
