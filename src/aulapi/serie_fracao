package expi;//Escreva um programa que mostra a soma dos 10000 primeiros termos da seguinte s√©rie:soma = 1/1 + 1/3 + 1/5 + 1/7 + 1/9 + ...

public class serie_fracao {
    public static void main(String[] args) {
        double n = 1 ;
        double soma = 0;
        for (int i = 1; i <=10000; i++){
            soma = soma + (1/n);
            n = n+2;
                    
        }
        System.out.println("Soma :"+soma);
    }
    
}
