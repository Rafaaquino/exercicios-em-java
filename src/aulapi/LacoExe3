package exercicios;

/**
 *
 * @author rafael.eraquino
 */
public class LacoExe3 {
     public static void main(String args[]){
        
        
        int regressiva = 0;
        
        for(int i = 100; i >= regressiva; i = i -5 ){
            
            System.out.println("regressiva "+ i);
        }
        
    }
}
