
package switchcase;

import java.util.Scanner;

/**
 *
 * @author upper
 */
public class SwitchCase5 {
    public static void main(String[]args){
        Scanner leitor = new Scanner(System.in);
        
        
        int mes;
        
        System.out.println("Digite o número de um mês para saber a se é de alta temporada: ");
        mes = leitor.nextInt();
        
        
        switch(mes){
            
            case 3:
            case 4:    
            case 5:
            case 8:
            case 9:
            case 10:
            case 11:    
                System.out.println("Mês de baixa temporada");
                break;
            case 1:
            case 2:    
            case 7:
            case 6:
            case 12:
                
                System.out.println("Mês de alta temporada");
                break;
             
            default:    
               System.out.println("Número inválido");
               
        }
        
    }
}
