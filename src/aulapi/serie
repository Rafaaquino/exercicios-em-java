package expi;

//Escreva um programa que mostra a soma dos 10000 primeiros termos da seguinte série: soma = 1+3+5+7...


public class soma_series {
    public static void main(String[] args) {
        int n = 1 ;
        int soma = 0;
        for (int i = 1; i <=10000; i++){
            soma = soma + n;
            n = n+2;
            
        }
        System.out.println("Soma :"+soma);
    }
}
