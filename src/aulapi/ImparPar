
package exerciciopi;

import java.util.Scanner;

/**
 *
 * @author upper
 */
public class ImparPar {
    public static void main(String[]args){
        
        Scanner leitor = new Scanner(System.in);
        
        System.out.print("Jogador Um Escreva um numero: ");        
        int jg1 = leitor.nextInt();
        
        System.out.print("Jogador Dois Escreva um numero: ");        
        int jg2 = leitor.nextInt();
        
        if (jg1 % 2 == 0){
            
            System.out.println("Par ganhou");
        
        }else{
            
            System.out.println("Impar ganhou");
        }
        
        
       
    }
}
