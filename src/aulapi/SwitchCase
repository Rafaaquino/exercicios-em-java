public class SwitchCase {
    public static void main(String[]args){
        
        char grande ='c';
        
        switch(grande){
            
            case 'a':
                System.out.println("Excelente!");
                break;
            case 'b':
            case 'c':
                System.out.println("Bem feito!");
                break;    
            case 'd':
                System.out.println("VocÃª passou");
                break; 
            case 'e':
                System.out.println("Melhor tentar novamente");
                break;
            default:    
                System.out.println("Melhor tentar novamente");
        }
        
    }
}
