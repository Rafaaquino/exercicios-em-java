
package exerciciopi;

import java.util.Scanner;

/**
 *
 * @author upper
 */
public class Jokenpo {
    public static void main(String[]args){
        
        Scanner leitor = new Scanner(System.in);
        
        int papel = 1, tesoura = 2, pedra = 3, r,a;
   
        
        System.out.println("Escolha  papel = 1, tesoura = 2, pedra = 3: ");
        r = leitor.nextInt();
        
        System.out.println("Escolha  papel = 1, tesoura = 2, pedra = 3: ");
        a = leitor.nextInt();
        
        if(r == papel && a == tesoura){
        
            System.out.println("Tesoura ganhou");
        }else if(r == papel && a == pedra){
            
             System.out.println("Papel ganhou");
             
        }else if(r == tesoura && a == pedra){
            
             System.out.println("Pedra ganhou");
             
        }else if(r == pedra && a == papel){
            
             System.out.println("Papel ganhou");
             
        }else {System.out.println("Empate");}
    }
}
